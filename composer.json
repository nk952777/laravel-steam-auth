{
    "name": "invisnik/laravel-steam-auth",
    "description": "Laravel Steam Auth",
    "keywords": ["steam", "laravel", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nikita Brytkov",
            "email": "invis.nik@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/http": "^10.0",
        "illuminate/routing": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Invisnik\\LaravelSteamAuth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Invisnik\\LaravelSteamAuth\\SteamServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
